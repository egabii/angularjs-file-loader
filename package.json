{
  "name": "file-uploader",
  "private": true,
  "version": "1.0.0",
  "description": "A starter project for AngularJS",
  "repository": "",
  "license": "MIT",
  "dependencies": {
    "angular": "^1.7.5",
    "jquery": "^3.3.1",
    "angular-loader": "^1.7.5",
    "angular-route": "^1.7.5",
    "html5-boilerplate": "0.0.1"
  },
  "devDependencies": {
    "angular-mocks": "^1.7.5",
    "cpx": "^1.5.0",
    "http-server": "^0.11.1"
  },
  "scripts": {
    "postinstall": "npm run copy-libs",
    "update-deps": "npm update",
    "postupdate-deps": "npm run copy-libs",
    "copy-libs": "cpx \"node_modules/{jquery,angular,angular-*,html5-boilerplate/dist}/**/*\" app/lib -C",
    "prestart": "npm install",
    "start": "http-server -a localhost -p 8000 -c-1 ./app",
    "pretest": "npm install"
  }
}
